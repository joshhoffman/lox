fun fib(n) {
    if (n <= 1) {
        return n;
    }
    return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 10; i = i + 1) {
    print fib(i);
}

var i = 0;
while (i < 10) {
    print fib(i);
    i = i + 1;
}

